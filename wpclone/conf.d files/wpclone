<VirtualHost *:80>
        ServerAdmin webmaster@mydomain.com
        ServerName pythonindia.com
        ServerAlias pythonindia.com
        WSGIScriptAlias /wpclone var/www/wpclone/wpclone/wsgi.py

        Alias /static/ /var/www/wpclone/static/
        <Location "/static/">
            Options -Indexes
        </Location>
</VirtualHost>